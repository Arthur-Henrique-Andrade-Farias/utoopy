{
  "name": "utoopy-monorepo",
  "private": true,
  "scripts": {
    "backend":  "npm --prefix server run dev",   
    "frontend": "npm --prefix client run serve", 
    "dev":      "concurrently --names \"back,front\" --raw \"npm run backend\" \"npm run frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}
